<UserControl x:Class="DashCode.Infrastructure.Controls.Explorer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DashCode.Infrastructure.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Name="explorerWindow">
    <StackPanel Background="{StaticResource Element2PlanBackgroundColor}">
        <TextBlock Padding="4" Background="{StaticResource ElementForegroundColor}" Foreground="{StaticResource MutedTextColor}" Text="{Binding NameText, ElementName=explorerWindow}"/>
        <ContentPresenter Content="{Binding Items, ElementName=explorerWindow}" Margin="4"/>
    </StackPanel>
</UserControl>
